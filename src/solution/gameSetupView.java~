class GameSetupView()
{
	public static void main(String[] args )
	{
		gameSetupView program = new gameSetupView();
		SwingUtilities.invokeLater(program::run);
	}
	
	private run()
	{
		JFrame w = new JFrame();
    	w.setDefaultCloseOperation(w.EXIT_ON_CLOSE);
		w.add(new PlayerBox()));
		w.pack();
		w.setLocationByPlatform(true);
		w.setVisible(true);	
	}
}
